{
  "extends": ["taro/react"],
  "rules": {
    "import/first": "off",
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "no-console": "off",
    "react/prop-types": [
      "warn"
    ],
    "react/jsx-curly-brace-presence": "off",
    "react/jsx-uses-react": "off",
    "react/jsx-indent-props": "off",
    "react/react-in-jsx-scope": "off"
  }
}
